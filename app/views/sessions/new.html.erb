<h1>Sign in </h1>

<%= form_with url:sign_in_path, data: {turbo: false} do |form| %>
<div class="mb-3">
    <%= form.label :email %>
    <%= form.email_field :email, class: "form-control", placeholder: "user@example.com" %>
</div>

<div class="mb-3">
    <%= form.label :password %>
    <%= form.password_field :password, class: "form-control", placeholder: "password" %>
   <!--- <%= link_to "Forgot your password?", password_reset_path %>--->
    <div class="mb-3">
    </div>
</div>

<div class="mb-3">
    <%= form.submit "Sign In", class: "btn btn-primary" %>
</div>
<%end%>
    <%= link_to "Don't have an account yet?", sign_up_path %>


